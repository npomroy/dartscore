<div class="container">
    <div class="row">
        <div class="col-md-8 col-md-offset-2">
            <% @games.each do |game| %>
                <div class="well">
                    <%= link_to game_path(id: game.id) do %>
                        <h4><%= game.starting_score %> at: </h4>
                    <% end %>
                </div>
            <% end %>
        </div>
    </div>
</div>